{
  "expo": {
    "name": "Thermal Polaroid",
    "slug": "rc-pos",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/polaroid.png",
    "scheme": "ThermalPolaroid",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/polaroid.png",
        "backgroundImage": "./assets/images/polaroid.png",
        "monochromeImage": "./assets/images/polaroid.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.carsonlee.ThermalPolaroid",
      "versionCode": 13,
      "permissions": [
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.CAMERA",
        "android.permission.LOCAL_NETWORK",
        "android.permission.REQUEST_INSTALL_PACKAGES"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/polaroid.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-sqlite",
      "expo-secure-store",
      "./plugins/with-network-security-config.js"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8ef9273c-23a1-4ca2-888a-8108465d2d0b"
      }
    }
  }
}